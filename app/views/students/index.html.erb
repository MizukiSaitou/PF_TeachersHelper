<table>
  <tr>
    <th>生徒名</th>
    <th>性別</th>
    <th>学年</th>
    <th>学校名</th>
    <th>電話番号</th>
  </tr>
  
  <% @students.each do |student| %>
  
  <tr>
    <td><%= student.name %></td>
    <td><%= student.gender %></td>
    <td><%= student.grade %></td>
    <td><%= student.school %></td>
    <td><%= student.phone_number %></td>
  </tr>
  
  <% end %>
</table>


<%= paginate @students %>